    setrate 1000

    set CPU processor1
    
    read RAM_X CPU "RAM_X"
    read RAM_Y CPU "RAM_Y"
    read RAM_WIDTH CPU "RAM_WIDTH"
    read RAM_SIZE CPU "RAM_SIZE"

    op div RAM_SIZE RAM_SIZE 4

    set i 0
loop:
    op mod x i RAM_WIDTH
    op add x x RAM_X

    op idiv y i RAM_WIDTH
    op add y y RAM_Y

    getblock building ram x y
    control enabled ram false

    op add i i 1
    jump loop lessThan i RAM_SIZE

    setrate 1
    stop
